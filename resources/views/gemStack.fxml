<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<fx:root fx:id="gemStack" type="AnchorPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
         maxHeight="120.0" minHeight="120.0" maxWidth="290.0" minWidth="290.0" prefHeight="120.0" prefWidth="290.0">
    <Button fx:id="diamondButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="10.0" layoutY="0.0" styleClass="gem-diamond,gem-button" onMouseClicked="#onDiamondClicked"/>
    <Button fx:id="emeraldButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="110.0" layoutY="0.0" styleClass="gem-emerald,gem-button" onMouseClicked="#onEmeraldClicked"/>
    <Button fx:id="rubyButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="210.0" layoutY="0.0" styleClass="gem-ruby,gem-button" onMouseClicked="#onRubyClicked"/>
    <Button fx:id="sapphireButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="10.0" layoutY="70.0" styleClass="gem-sapphire,gem-button" onMouseClicked="#onSapphireClicked"/>
    <Button fx:id="onyxButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="110.0" layoutY="70.0" styleClass="gem-onyx,gem-button" onMouseClicked="#onOnyxClicked"/>
    <Button fx:id="jokerButton" prefHeight="60.0" prefWidth="100.0"
            layoutX="210.0" layoutY="70.0" styleClass="gem-joker,gem-button" onMouseClicked="#onJokerClicked"/>
</fx:root>
